DATABASE_URL=sqlite:///./agenticai.db
ENVIRONMENT=development
HOST=127.0.0.1
PORT=8000
LOG_LEVEL=INFO
BUS_BACKEND=inmemory
TASK_API_AUTH_TOKEN=replace-me
# Set when BUS_BACKEND=redis
# REDIS_URL=redis://localhost:6379/0
# Optional Redis startup fallback toggle (false by default)
# BUS_REDIS_FALLBACK_TO_INMEMORY=false
# Optional dev/local opt-out for task API auth
# ALLOW_INSECURE_TASK_API=true
# Set to enforce Telegram webhook origin validation
# TELEGRAM_WEBHOOK_SECRET=replace-me
# Optional dev/local opt-out for Telegram webhook auth
# ALLOW_INSECURE_TELEGRAM_WEBHOOK=true
