DATABASE_URL=sqlite:///./agenticai.db
ENVIRONMENT=development
HOST=127.0.0.1
PORT=8000
LOG_LEVEL=INFO
BUS_BACKEND=inmemory
# Global toggle for API endpoint rate limiting middleware (false by default).
# ENABLE_RATE_LIMITING=true
# Max requests per window for Telegram webhook ingress.
# TELEGRAM_WEBHOOK_RATE_LIMIT_REQUESTS=60
# TELEGRAM_WEBHOOK_RATE_LIMIT_WINDOW_SECONDS=60
# Max requests per window for task creation API.
# TASK_CREATE_RATE_LIMIT_REQUESTS=30
# TASK_CREATE_RATE_LIMIT_WINDOW_SECONDS=60
# JWT secret used to validate bearer tokens on /v1/*.
TASK_API_JWT_SECRET=replace-me
# Audience required in JWT aud claim (default: agenticai-v1).
# TASK_API_JWT_AUDIENCE=agenticai-v1
# Supported algorithm (default and only supported value: HS256).
# TASK_API_JWT_ALGORITHM=HS256
# Set when BUS_BACKEND=redis
# REDIS_URL=redis://localhost:6379/0
# Optional Redis startup fallback toggle (false by default)
# BUS_REDIS_FALLBACK_TO_INMEMORY=false
# Set to enforce Telegram webhook origin validation
# TELEGRAM_WEBHOOK_SECRET=replace-me
# Optional dev/local opt-out for Telegram webhook auth
# ALLOW_INSECURE_TELEGRAM_WEBHOOK=true
